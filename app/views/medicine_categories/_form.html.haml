= form_for @medicine_category do |f|
  - if @medicine_category.errors.any?
    #error_explanation
      %h2= "#{pluralize(@medicine_category.errors.count, "error")} prohibited this medicine_category from being saved:"
      %ul
        - @medicine_category.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :medicine_id
    = f.text_field :medicine_id
  .field
    = f.label :category_id
    = f.text_field :category_id
  .actions
    = f.submit 'Save'
